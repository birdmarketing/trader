<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
	<group id="default">
		<job instance="BirdMarketing\Trader\Cron\Import" method="execute" name="birdmarketing_trader_import">
			<schedule>* * * * *</schedule>
		</job>
		<job instance="BirdMarketing\Trader\Cron\Export" method="execute" name="birdmarketing_trader_export">
			<schedule>* * * * *</schedule>
		</job>
	</group>
</config>